<span id="cb15-57"><a href="#cb15-57" aria-hidden="true" tabindex="-1"></a>     <span class="at">xlab=</span><span class="st">&quot;total products released in year&quot;</span>)</span>
<span id="cb15-58"><a href="#cb15-58" aria-hidden="true" tabindex="-1"></a><span class="fu">invisible</span>(<span class="fu">dev.off</span>())</span>
<span id="cb15-59"><a href="#cb15-59" aria-hidden="true" tabindex="-1"></a></span>
<span id="cb15-60"><a href="#cb15-60" aria-hidden="true" tabindex="-1"></a><span class="co"># extract the month of each kill</span></span>
<span id="cb15-61"><a href="#cb15-61" aria-hidden="true" tabindex="-1"></a>m <span class="ot">=</span> <span class="fu">months</span>(dead<span class="sc">$</span>Ended)</span>
<span id="cb15-62"><a href="#cb15-62" aria-hidden="true" tabindex="-1"></a><span class="co"># sort by chronological order, not alphabetical</span></span>
<span id="cb15-63"><a href="#cb15-63" aria-hidden="true" tabindex="-1"></a>m_fac <span class="ot">=</span> <span class="fu">factor</span>(m, <span class="at">levels =</span> month.name)</span>
<span id="cb15-64"><a href="#cb15-64" aria-hidden="true" tabindex="-1"></a><span class="co"># count by month</span></span>
<span id="cb15-65"><a href="#cb15-65" aria-hidden="true" tabindex="-1"></a>months <span class="ot">&lt;-</span> <span class="fu">table</span>(<span class="fu">sort</span>(m_fac))</span>
<span id="cb15-66"><a href="#cb15-66" aria-hidden="true" tabindex="-1"></a><span class="co"># shutdowns by month are imbalanced:</span></span>
<span id="cb15-67"><a href="#cb15-67" aria-hidden="true" tabindex="-1"></a><span class="fu">print</span>(<span class="fu">chisq.test</span>(months))</span>
<span id="cb15-68"><a href="#cb15-68" aria-hidden="true" tabindex="-1"></a><span class="co"># and visibly so:</span></span>
<span id="cb15-69"><a href="#cb15-69" aria-hidden="true" tabindex="-1"></a><span class="fu">png</span>(<span class="at">file=</span><span class="st">&quot;~/wiki/images/google/shutdownsbymonth.png&quot;</span>, <span class="at">width =</span> <span class="fl">1.5</span><span class="sc">*</span><span class="dv">480</span>, <span class="at">height =</span> <span class="dv">1</span><span class="sc">*</span><span class="dv">480</span>)</span>
<span id="cb15-70"><a href="#cb15-70" aria-hidden="true" tabindex="-1"></a><span class="fu">plot</span>(months)</span>
<span id="cb15-71"><a href="#cb15-71" aria-hidden="true" tabindex="-1"></a><span class="fu">invisible</span>(<span class="fu">dev.off</span>())</span>
