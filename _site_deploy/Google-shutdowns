<span id="cb15-48"><a href="#cb15-48" aria-hidden="true" tabindex="-1"></a><span class="fu">hist</span>(dead<span class="sc">$</span>Ended, <span class="at">breaks=</span><span class="fu">seq.Date</span>(<span class="fu">as.Date</span>(<span class="st">&quot;2005-01-01&quot;</span>), <span class="fu">as.Date</span>(<span class="st">&quot;2014-01-01&quot;</span>), <span class="st">&quot;years&quot;</span>),</span>
<span id="cb15-49"><a href="#cb15-49" aria-hidden="true" tabindex="-1"></a>                   <span class="at">main=</span><span class="st">&quot;shutdowns per year&quot;</span>, <span class="at">xlab=</span><span class="st">&quot;Year&quot;</span>)</span>
