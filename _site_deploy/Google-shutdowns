<span id="cb15-210"><a href="#cb15-210" aria-hidden="true" tabindex="-1"></a></span>
<span id="cb15-211"><a href="#cb15-211" aria-hidden="true" tabindex="-1"></a>coxProbability <span class="ot">&lt;-</span> <span class="cf">function</span>(cm, d, t) {</span>
<span id="cb15-212"><a href="#cb15-212" aria-hidden="true" tabindex="-1"></a>    x <span class="ot">&lt;-</span> <span class="fu">survfit</span>(cm, <span class="at">newdata=</span>d)</span>
<span id="cb15-213"><a href="#cb15-213" aria-hidden="true" tabindex="-1"></a>    p <span class="ot">&lt;-</span> x<span class="sc">$</span>surv[<span class="fu">Position</span>(<span class="cf">function</span>(a) a<span class="sc">&gt;</span>t, x<span class="sc">$</span>time)]</span>
<span id="cb15-214"><a href="#cb15-214" aria-hidden="true" tabindex="-1"></a>    <span class="cf">if</span> (<span class="fu">is.null</span>(p)) { <span class="fu">coxProbability</span>(d, (t<span class="dv">-1</span>)) } <span class="cf">else</span> {<span class="cf">if</span> (<span class="fu">is.na</span>(p)) p <span class="ot">&lt;-</span> <span class="dv">0</span>}</span>
<span id="cb15-215"><a href="#cb15-215" aria-hidden="true" tabindex="-1"></a>    p</span>
<span id="cb15-216"><a href="#cb15-216" aria-hidden="true" tabindex="-1"></a>    }</span>
<span id="cb15-217"><a href="#cb15-217" aria-hidden="true" tabindex="-1"></a>randomCoxPredictionAccuracy <span class="ot">&lt;-</span> <span class="cf">function</span>(gb, indices) {</span>
<span id="cb15-218"><a href="#cb15-218" aria-hidden="true" tabindex="-1"></a>    g <span class="ot">&lt;-</span> gb[indices,]</span>
<span id="cb15-219"><a href="#cb15-219" aria-hidden="true" tabindex="-1"></a>    cmodel <span class="ot">&lt;-</span> cmodel <span class="ot">&lt;-</span> <span class="fu">coxph</span>(<span class="fu">Surv</span>(Days, Dead) <span class="sc">~</span> Acquisition <span class="sc">+</span> FLOSS <span class="sc">+</span> Profit <span class="sc">+</span> Social <span class="sc">+</span> Type <span class="sc">+</span> DeflatedHits,</span>
<span id="cb15-220"><a href="#cb15-220" aria-hidden="true" tabindex="-1"></a>                <span class="at">data =</span> g)</span>
<span id="cb15-221"><a href="#cb15-221" aria-hidden="true" tabindex="-1"></a></span>
<span id="cb15-222"><a href="#cb15-222" aria-hidden="true" tabindex="-1"></a>    predictions <span class="ot">&lt;-</span> <span class="cn">NULL</span>;</span>
<span id="cb15-223"><a href="#cb15-223" aria-hidden="true" tabindex="-1"></a>    <span class="cf">for</span> (i <span class="cf">in</span> <span class="dv">1</span><span class="sc">:</span><span class="fu">nrow</span>(google)) { predictions[i] <span class="ot">&lt;-</span> <span class="fu">coxProbability</span>(cmodel, google[i,], totals[i]) }</span>
<span id="cb15-224"><a href="#cb15-224" aria-hidden="true" tabindex="-1"></a></span>
<span id="cb15-225"><a href="#cb15-225" aria-hidden="true" tabindex="-1"></a>    <span class="fu">return</span>(<span class="fu">sum</span>((predictions<span class="sc">&lt;</span><span class="fl">0.50</span>) <span class="sc">==</span> google<span class="sc">$</span>Dead) <span class="sc">/</span> <span class="fu">nrow</span>(google))</span>
<span id="cb15-226"><a href="#cb15-226" aria-hidden="true" tabindex="-1"></a>    }</span>
<span id="cb15-227"><a href="#cb15-227" aria-hidden="true" tabindex="-1"></a><span class="fu">print</span>(<span class="fu">randomCoxPredictionAccuracy</span>(google, <span class="dv">1</span><span class="sc">:</span><span class="fu">nrow</span>(google)))</span>
<span id="cb15-228"><a href="#cb15-228" aria-hidden="true" tabindex="-1"></a>coxBs <span class="ot">&lt;-</span> <span class="fu">boot</span>(<span class="at">data=</span>google, <span class="at">statistic=</span>randomCoxPredictionAccuracy, <span class="at">R=</span><span class="dv">200</span>, <span class="at">parallel=</span><span class="st">&quot;multicore&quot;</span>, <span class="at">ncpus=</span><span class="dv">4</span>)</span>
